    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "custom",
            "name": "Open core dump",
            "targetCreateCommands": [
                "target create ${workspaceFolder}/target/debug/coredump-test --core ${input:coreFileName}"
            ],
        }
    ],
    "inputs": [
        {
            "id": "coreFileName",
            "type": "promptString",
            "description": "Enter core file path"
        }
    ]
}
